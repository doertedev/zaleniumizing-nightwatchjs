{
	"src_folders": ["."],
	"selenium": {
 		"start_process": false,
		"host" : "zalenium-tmp.qa-instana.io",
		"port" : 30000,
    "cli_args" : {
      "webdriver.chrome.driver" : "/usr/local/bin/chromedriver",
      "webdriver.gecko.driver" : "/usr/local/bin/geckodriver"
    }
	},
	"test_settings": {
		"default": {
      "launch_url": "http://zalenium-tmp.qa-instana.io:30000",
      "selenium_port": 30000,
      "selenium_host": "zalenium-tmp.qa-instana.io",
			"desiredCapabilities": {
				"tz": "Europe/Berlin",
				"idleTimeout": 150,
				"seleniumProtocol": "WebDriver",
				"browserName": "chrome",
				"javascriptEnabled": true,
				"acceptSslCerts": true
			},
 			"disable_colors": true,
			"screenshots": {
				"enabled": false
			}
		},
		"ff" : {
			"desiredCapabilities": {
				"tz": "Europe/Berlin",
				"idleTimeout": 150,
				"marionette": false,
				"browserName": "firefox"
			}
		}
	}
}
